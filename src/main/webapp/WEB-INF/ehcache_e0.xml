<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
	updateCheck="true" monitoring="autodetect" dynamicConfig="true">

	<!-- <diskStore path="/ehcache/agriQuiz" /> -->

	<cache name="agriQuizJsonCache" maxElementsInMemory="6"
		eternal="false" timeToIdleSeconds="120" timeToLiveSeconds="14400"
		overflowToDisk="true" diskPersistent="false"
		diskExpiryThreadIntervalSeconds="14400" memoryStoreEvictionPolicy="LRU" />					
</ehcache>





 